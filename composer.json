{
  "repositories": {
    "hmvc": {
      "type": "git",
      "url": "https://github.com/getwws/getw.git"
    }
  },
  "minimum-stability": "dev",
  "require": {
    "php": ">=5.5.9",
    "getwws/getw": "dev-master",
    "respect/validation": "1.1.x-dev",
    "mustache/mustache":"*",
    "nikic/fast-route":"1.3.0"
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "autoload": {
    "files": [
      "system/helpers.php"
    ],
    "psr-4": {
      "system\\": "system/",
      "app\\": "app/"
    }
  }
}
