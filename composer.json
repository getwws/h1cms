{
  "repositories": {
    "getwws": {
      "type": "git",
      "url": "https://github.com/getwws/getw.git"
    }
  },
  "minimum-stability": "dev",
  "require": {
    "php": ">=5.5.9",
    "ext-gd": "*",
    "getwws/getw": "dev-master",
    "maximebf/debugbar": "1.15.0",
    "mustache/mustache": "*",
    "nikic/fast-route": "1.3.0",
    "symfony/var-dumper": "^2.6|^3"
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "autoload": {
    "files": [
      "system/helpers.php"
    ],
    "psr-4": {
      "system\\": "system/",
      "app\\": "app/"
    }
  }
}
